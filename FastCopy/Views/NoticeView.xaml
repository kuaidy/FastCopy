<Window x:Class="FastCopy.Views.NoticeView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FastCopy.Views"
        mc:Ignorable="d"
        Title="新消息通知" Height="Auto" MinHeight="100" Width="300" Loaded="Window_Loaded" ResizeMode="NoResize">
    <Grid>
        <TextBlock Text="{Binding NoticeInfo}" TextWrapping="Wrap"></TextBlock>
    </Grid>
</Window>
